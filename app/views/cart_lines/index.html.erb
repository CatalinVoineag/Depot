   <div class="account-in">
   	 <div class="container">
		  <div class="check_box">	 
		<div class="col-md-9 cart-items">
			 <h1>My Shopping Bag (<%=cart_lines_count(current_cart) %>)</h1>
			
			<% @cart_lines.each do |line| %>
				<%= simple_form_for line, url: edit_quantity_path(line), method: :post do |f| %>
					 <div class="cart-header">
					 	<%= link_to cart_line_path(line), method: :delete do %>
						 	<div class="close1"> </div>
						<% end %>
						   <div class="cart-sec simpleCart_shelfItem">
								<div class="cart-item cyc">
									 <img src="images/m3.jpg" class="img-responsive" alt=""/>
								</div>
							   <div class="cart-item-info">
								<h3><a href="#"><%= line.product.name %></a></h3>
								<ul class="qty">
									<li><p>Qty : <%= f.input :quantity, class: 'form-control', label: false %></p></li>
									<li><p>Price : <%= price_tag(line) %></p></li>
								</ul>
						
							   </div>
							   <div class="clearfix"></div>
						    </div>
					 </div>
				<% end %>
			<% end %>
			
			
		 </div>
		 <div class="col-md-3 cart-total">
			 <%= link_to "Continue Shoping", products_path, class:'continue' %>
			 <div class="price-details">
				 <h3>Price Details</h3>
				 <span>Total</span>
				 <span class="total1"><%= total_price(current_cart) %></span>
				 <span>Discount</span>
				 <span class="total1">---</span>
				 <span>Delivery Charges</span>
				 <span class="total1">0</span>
				 <div class="clearfix"></div>				 
			 </div>	
			 <ul class="total_price">
			   <li class="last_price"> <h4>TOTAL</h4></li>	
			   <li class="last_price"><span><%= total_price(current_cart) %></span></li>
			   <div class="clearfix"> </div>
			 </ul>
			 <div class="clearfix"></div>
			 
			 <%= simple_form_for :order, url: create_order_path do |f| %>
			   <%= f.submit "Place Order", class:'order' %>
			 <% end %>



			 




			 <div class="total-item">
				 <h3>OPTIONS</h3>
				 <h4>COUPONS</h4>
				 <a class="cpns" href="#">Apply Coupons</a>
				 <p><a href="#">Log In</a> to use accounts - linked coupons</p>
			 </div>
			</div>
			<div class="clearfix"></div>
	     </div>
	  </div>
   </div>

   <div class="footer">
   	 <div class="container">
   		<div class="cssmenu">
		   <ul>
			<li class="active"><a href="#">Privacy</a></li>
			<li><a href="#">Terms</a></li>
			<li><a href="#">Shop</a></li>
			<li><a href="#">About</a></li>
			<li><a href="contact.html">Contact</a></li>
		  </ul>
		</div>
		<ul class="social">
			<li><a href=""> <i class="instagram"> </i> </a></li>
			<li><a href=""><i class="fb"> </i> </a></li>
			<li><a href=""><i class="tw"> </i> </a></li>
	    </ul>
	    <div class="clearfix"></div>
	    
   	</div>
   </div>
